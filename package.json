{
  "name": "@minisheeep/mp-three-examples",
  "version": "1.0.3",
  "description": "three.js examples for mini-program",
  "author": "minisheep",
  "type": "module",
  "homepage": "https://www.minisheep.cn",
  "repository": {
    "type": "git",
    "url": "https://github.com/minisheeep/mp-three-examples"
  },
  "module": "./dist/official/index.js",
  "types": "./dist/official/index.d.ts",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "exports": {
    ".": {
      "import": "./dist/official/index.js",
      "types": "./dist/official/index.d.ts"
    },
    "./official/*": "./dist/official/*"
  },
  "files": [
    "dist",
    "!dist/stats.html",
    "*.md"
  ],
  "scripts": {
    "dev:web": "vite",
    "dev": "vite build -w",
    "prebuild": "rimraf ./dist",
    "build": "vite build",
    "lint": "eslint src/**/* --fix",
    "format": "prettier --write ./src/**/*.{js,jsx,ts,tsx,vue}",
    "prepublishOnly": "npm run build"
  },
  "keywords": [],
  "license": "MIT",
  "packageManager": "pnpm@10.5.2",
  "lint-staged": {
    "*.{json}": [
      "prettier --write"
    ],
    "*.{js,jsx,ts,tsx,vue}": [
      "eslint",
      "prettier --write"
    ]
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "vue": "^3.4.3"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "vue": {
      "optional": true
    }
  },
  "dependencies": {
    "lodash-es": "^4.17.21",
    "react": "^19.0.0",
    "three": "^0.174.0",
    "three-bvh-csg": "^0.0.16",
    "three-gpu-pathtracer": "^0.0.23",
    "three-mesh-bvh": "^0.9.0",
    "three-subdivide": "^1.1.5",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@minisheep/mini-program-polyfill-core": "^1.1.4",
    "@minisheep/three-platform-adapter": "^1.2.7",
    "@rollup/plugin-babel": "^6.0.4",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.13.10",
    "@types/stats.js": "^0.17.3",
    "@types/three": "^0.174.0",
    "@typescript-eslint/parser": "^8.26.1",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitejs/plugin-vue": "^5.2.2",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.0",
    "cheerio": "^1.0.0",
    "eslint": "^9.22.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-vue": "^10.0.0",
    "glob": "^11.0.1",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "lil-gui": "^0.20.0",
    "miniprogram-api-typings": "^4.0.5",
    "prettier": "^3.5.3",
    "rollup-plugin-visualizer": "^5.14.0",
    "sass": "^1.85.1",
    "stats.js": "^0.17.0",
    "vite": "^6.2.2",
    "vite-plugin-dts": "^4.5.3",
    "vite-plugin-glsl": "^1.3.3",
    "vite-plugin-static-copy": "^2.3.0"
  }
}
